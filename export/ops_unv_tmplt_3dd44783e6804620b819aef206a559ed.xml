# Used modules
import datetime
import fnmatch

from google.cloud import storage, exceptions
import logging
import os
version = "3.0"


# -- logging settings DEBUG, INFO, WARNING, ERROR, CRITICAL
# logging.basicConfig(level=logging.${ops_gcs_loglevel},
# format=' %(asctime)s - %(levelname)s - %(message)s')
logging.basicConfig(level="${ops_gcs_loglevel}",
                    format=' %(asctime)s - %(levelname)s - %(message)s')


def main():
    set_variables()  # from Universal Task Form or Command Line Input
    sb_gcs_con = SbGcs("${_scriptPath('${ops_gcs_keyfile}')}",
                       "${ops_gcs_useproxy}",
                       "${ops_gcs_proxy_type}",
                       "${_credentialUser('${ops_gcs_proxy_cred}')}",
                       "${_credentialPwd('${ops_gcs_proxy_cred}')}",
                       "${ops_gcs_proxy_host}",
                       "${ops_gcs_proxy_port}")
    sb_gcs_con.upload_all_blobs("${ops_gcs_bucket_name}",
                                "${ops_gcs_source_file_name}",
                                "${ops_gcs_destination_blob_name}",
                                "${ops_gcs_overwrite}",
                                "${ops_gcs_operation}")


def set_variables():
    logging.info(
        "Executing version " + version + " with the following parameters")
    logging.info('####### Parameters:')  # print out all parameters
    logging.info(r'Keyfile: ${ops_gcs_keyfile}')
    logging.info(r'bucket name: ${ops_gcs_bucket_name}')
    logging.info(r'file to upload: ${ops_gcs_source_file_name}')
    logging.info(r'destination blob_file_name: '
                 r'${ops_gcs_destination_blob_name}')
    logging.info(r'Use Proxy: ${ops_gcs_useproxy}')
    logging.info(r'Proxy Host: ${ops_gcs_proxy_host}')
    logging.info(r'Proxy Port: ${ops_gcs_proxy_port}')
    logging.info(r"Proxy User: ${_credentialUser('${ops_gcs_proxy_cred}')} ")
    logging.info(r"Proxy Password: ${_credentialPwd('${ops_gcs_proxy_cred}')}")


class SbGcs:
    """Stonebranch Google Storage Class for python

    This class is written for file operations
    to and from Google cloud

    Attributes:
        _storage_client: keeps the resource identifier
        _library_version: keeps the class library version
    """

    def __init__(self,
                 key_file,
                 use_proxy="0",
                 proxy_type="http",
                 proxy_user=None,
                 proxy_passwd=None,
                 proxy_host=None,
                 proxy_port=None):
        self._storage_client = None
        self.connect_gcs(key_file,
                         use_proxy,
                         proxy_type,
                         proxy_user,
                         proxy_passwd,
                         proxy_host,
                         proxy_port)

    def __del__(self):
        self._storage_client = None

    def connect_gcs(self,
                    key_file,
                    use_proxy="0",
                    proxy_type="http",
                    proxy_user=None,
                    proxy_passwd=None,
                    proxy_host=None,
                    proxy_port=None):
        """Connects to cloud.

        Connects to the cloud with or without proxy
        Assigns the resource id to _storage_client

        Args:
            key_file: Aws access key file
            use_proxy: proxy use flag, 0 or 1
            proxy_type: proxy type, http or https
            proxy_user: proxy user information
            proxy_passwd: proxy password information
            proxy_host: proxy host information
            proxy_port: proxy port information

        Returns:
            -

        Raises:
            -
        """
        # Make the connection to Google Cloud either via Proxy or direct
        if use_proxy in "Yes,true,True":
            # connection via Proxy
            logging.info(
                f'Google Cloud Connection with Proxy: '
                f'{proxy_host} using port: {proxy_port}')
            logging.debug('Establishing connection')
            if proxy_type == "https":
                os.environ['https_proxy'] = "https://%s:%s@%s:%s" % (
                    proxy_user, proxy_passwd, proxy_host, proxy_port)
            else:
                os.environ['http_proxy'] = "http://%s:%s" % (
                    proxy_host, proxy_port)
            # storage_client =
            # storage.Client.from_service_account_json(key_file)
        else:
            # connection without Proxy
            logging.info("Direct Connection without Proxy")
            # storage_client =
            # storage.Client.from_service_account_json(key_file)
        storage_client = storage.Client.from_service_account_json(key_file)
        self._storage_client = storage_client

    def upload_blob(self,
                    bucket_name,
                    source_file_name,
                    target_blob_name,
                    overwrite):
        """Uploads files to Google cloud.

        Uploads files to Google cloud

        Args:
            bucket_name: bucket name
            source_file_name: source file with unix pattern
            target_blob_name: target file name
            overwrite: overwrites if the value is "true",
                otherwise adds a timestamp ".%Y-%m-%dT%H:%M:%S%z"

        Returns:
            -

        Raises:
            -
        """
        if not target_blob_name.strip():
            dir_name, destination_blob_name = os.path.split(
                os.path.abspath(source_file_name))
        else:
            destination_blob_name = target_blob_name

        try:
            bucket = self._storage_client.get_bucket(bucket_name)
            blob = bucket.blob(destination_blob_name)
            if blob.exists():
                if overwrite in "true,True,replace,Overwrite":
                    blob.upload_from_filename(source_file_name)
                    logging.info(f'File {source_file_name} uploaded to '
                                 f'{bucket_name} as blob_file_name: '
                                 f'{destination_blob_name}.')
                elif overwrite in "wt,Timestamp":
                    timestamp = str(datetime.datetime.now().strftime(
                        ".%Y-%m-%dT%H:%M:%S%z"))
                    logging.info(
                        'File: %s exists and new one will be renamed '
                        'with timestamp %s' %
                        (source_file_name, timestamp))
                    blob.upload_from_filename(source_file_name + timestamp)
                    logging.info(f'File {source_file_name} '
                                 f'uploaded to {bucket_name} '
                                 f'as blob_file_name: {destination_blob_name}.')
                elif overwrite in "false,False,new":
                    logging.error(
                        'Blob: %s exists and will not be overwritten, '
                        'set overwrite flag' % destination_blob_name)
                    return False
                else:
                    logging.error('Unknown overwrite option, exiting')
                    exit(1)
            else:
                blob.upload_from_filename(source_file_name)
                logging.info(f'File {source_file_name} '
                             f'uploaded to {bucket_name} '
                             f'as blob_file_name: {destination_blob_name}.')
        except Exception as e:
            logging.error(f'Exception during upload_blob')
            if hasattr(e, 'message'):
                logging.error(f'{e.message}')
            else:
                logging.error(e)
            if hasattr(e, 'code'):
                logging.error(f'{e.code}')
                exit(e.code)
            else:
                exit(1)
        return True

    def upload_all_blobs(self,
                         bucket_name,
                         source_file_name,
                         destination_blob_name,
                         overwrite="true",
                         operation="copy"):
        try:
            dest_path, dest_filename = os.path.split(destination_blob_name)
            dir_name, filename = os.path.split(os.path.abspath(source_file_name))

            files = fnmatch.filter(os.listdir(dir_name), filename)
            logging.info(files)
            transfer_success = True
            for file in files:
                if dest_path:
                    dest_full_path = dest_path + "/" + file
                else:
                    dest_full_path = file
                exact_source_file_name = dir_name + "/" + file
                if self.upload_blob(bucket_name,
                                 exact_source_file_name,
                                 dest_full_path,
                                 overwrite) is True:
                    if operation == "move":
                        self.delete_file_on_disk(exact_source_file_name)
                else:
                    transfer_success = False
            if transfer_success is False:
                logging.error('There has been some errors!')
                exit(1)
        except Exception as e:
            logging.error(f'Exception during download_blob')
            if hasattr(e, 'message'):
                logging.error(f'{e.message}')
            else:
                logging.error(e)
            if hasattr(e, 'code'):
                logging.error(f'{e.code}')
                exit(e.code)
            else:
                exit(1)

    @staticmethod
    def delete_file_on_disk(file_name):
        """Delete a file that exists on disk.

        Deletes a file that exists on local disk

        Args:
            file_name: file name to be checked

        Returns:
            True if the file exists
            False if the file does not exist

        Raises:
            -
        """
        try:
            os.remove(file_name)
        except OSError as e:  # operation failed, report it back to the user
            logging.error("Error: %s - %s." % (e.filename, e.strerror))
            return False
        else:
            logging.info("File %s deleted successfully" % file_name)
            return True


